<div class="container">
  <!-- Banner Top-->
  <div class="row" data-aos="fade-up">
    <div class="col-xl-12 stretch-card grid-margin">
      <div class="card">
        <div class="position-relative cursor-pointer" onclick="window.open('/photos','_blank').focus">
          <img src="images/dashboard/banner2.jpeg" alt="banner" class="img-fluid" />
          <div class="banner-top">
            <h1 class="mb-0">{{shortTitle}}</h1>
            <h1 class="mb-2">
              Glimpses of Durga Puja, 2022 in Agartala
            </h1>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- End-->
  <!-- Headline Top with Latest News-->
  <div class="row" data-aos="fade-up">
    <div class="col-xl-8 stretch-card grid-margin">
      {{#list global}}
      <div class="position-relative" onclick="window.location.href = '/news?category=global&id={{id}}'">
        <img src="images/news/global/{{imageName}}" alt="banner" class="img-fluid" />
        <div class="banner-content">
          <div class="overlay">
            <div class="badge badge-danger fs-12 font-weight-bold mb-3">
              Top News
            </div>
            <h1 class="mb-0">{{shortTitle}}</h1>
            <h1 class="mb-2">
              {{title}}
            </h1>
            <div class="fs-12">
              <span class="mr-2">NEH </span>10 Minutes ago
            </div>
          </div>
        </div>
      </div>
      {{/list}}
    </div>
    <div class="col-xl-4 stretch-card grid-margin">
      <div class="card bg-dark text-white">
        <div class="card-body">
          <h2>Latest news</h2>
          {{#latestnews latestnews}}
          <div class="pr-3 news-a cursor-pointer" onclick="window.location.href = '/news?category={{newsCategory}}&id={{id}}'">
            <h5>{{title}}</h5>
            <div class="fs-12">
              <span class="mr-2">NEH </span>{{publishedAt}}
            </div>
          </div>
          <div class="rotate-img latest-news-home cursor-pointer" onclick="window.location.href = '/news?category={{newsCategory}}&id={{id}}'">
            <img src="images/news/{{newsCategory}}/{{imageName}}" alt="thumb" class="img-fluid img-lg" />
          </div>
          {{/latestnews}}
        </div>
      </div>
    </div>
  </div>
  <!-- End-->
